<extend name="Myapp/Tpl/Public/model.html"/>
<block name="title"><title>暗化操作信息修改</title></block>
<block name="content">
    <div style="margin-top:1%">
        <form action="__APP__/Seedlings/Seed/updatedarkening" method="post">
            <table class="table table-bordered">
                <tr style="height:45px">
                    <td colspan="5" align="center" bgcolor="#E9E9E9">暗化操作信息修改</td>
                </tr>
                <tr style="height:45px">
                    <td style="width: 150px;background-color:#E9E9E9">托盘批次号</td>
                    <td ><input class="form-control"  style="width: 250px;height: 30px" type="text" name='trayid'  value="<{$data.trayid}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">托盘数量</td>
                    <td><input class="form-control" style="width: 250px;height: 30px" type="text" name='trayamount'value="<{$data.trayamount}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">暗化地点</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='traysite'  value="<{$data.traysite}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">起始日期</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='startdate'  value="<{$data.startdate}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color: #E9E9E9">结束日期</td>
                    <td><input class="form-control" style="width: 250px;height: 30px" type="text" name='enddate'  value="<{$data.enddate}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9"></td>
                    <td><input type='hidden' name='id' value="<{$data.id}>"/><input class="btn btn-primary" type="submit" value="确认修改" onclick="return confirm('是否确认？')"/></td>
                </tr>
            </table>
        </form>
    </div>
</block>
