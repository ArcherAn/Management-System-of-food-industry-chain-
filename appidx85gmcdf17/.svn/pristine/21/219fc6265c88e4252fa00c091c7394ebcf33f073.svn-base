<extend name="Myapp/Tpl/Public/model.html"/>
<block name="title"><title>秧苗信息</title></block>
<block name="content">
    <script>
        function jump(){
            window.location="__APP__/Seedlings/Seedling/addseedinginfo";
        }
    </script>
    <div>
        <div style="height: 50px">
            <span style="margin-top: 5px;font-size: 28px">阅读器管理</span><span style="font-size: 14px;margin-top: 3px;">&nbsp;育秧管理平台</span>
        </div>
        <div style="background-color: #b1bab7;height: 30px">
            <span style="margin-top: 5px;float: left">主页&nbsp;>&nbsp;育秧&nbsp;>&nbsp;秧苗管理</span>
        </div>
        <div  style="margin-top: 10px;">
            <form action="__APP__/Seedlings/Seedling/seedseedinginfo" method="get">
                <span style="font-size: 16px;float: left">秧苗批次号：</span>
                <span style="float: left;margin-left: 10px"><input name="trayid" type="text" placeholder="请输入要搜索的秧苗批次号" class="form-control"style="width: 200px;height: 30px"></span>
                <span><button type="submit" class="btn btn-primary btn-sm" style="float: left;margin-left: 10px;">查询</button></span>
                <span><button type="button" class="btn btn-success btn-sm" style="float: left;margin-left: 10px" onclick="jump()">新增信息</button></span>
            </form>
        </div>
        <div>
            <table  class="table table-striped table-bordered" style="margin-top: 50px;align:center;">
                <thead>
                <tr>
                    <th>秧苗批次号</th>
                    <th>密度</th>
                    <th>株高</th>
                    <th>叶片数</th>
                    <th>叶片长度</th>
                    <th>叶片宽度</th>
                    <th>叶色情况</th>
                    <th>叶厚</th>
                    <th>根长</th>
                    <th>日期</th>
                    <th>操作</th>
                </tr>
                </thead>
                <tbody>
                <volist name='data' id='vo'>
                    <tr>
                        <td><{$vo.seedingid}></td>
                        <td><{$vo.density}></td>
                        <td><{$vo.high}>cm</td>
                        <td><{$vo.leafnumber}></td>
                        <td><{$vo.leaflength}>cm</td>
                        <td><{$vo.leafwidth}>cm</td>
                        <td><{$vo.leafcolor}></td>
                        <td><{$vo.leafthickness}></td>
                        <td><{$vo.rootlength}></td>
                        <td><{$vo.date}></td>
                        <td><a href="__APP__/Seedlings/Seedling/modifyseedinginfo/id/<{$vo.id}>">修改</a>|
                            <a href="__APP__/Seedlings/Seedling/deleteseedinginfo/id/<{$vo.id}>"onclick="return confirm('是否删除？')">删除</a>
                        </td>
                    </tr>
                </volist>
                </tbody>
            </table>
        </div>
        <div align="center"><{$page}></div>
    </div>
</block>
