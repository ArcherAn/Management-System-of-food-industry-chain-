<block name="diyStyle">
<style>
table, th, td {
	border: 0px solid blue;
	text-align: center;
}

table {
	margin-top: 2%;
	width: 100%;
}

td {
	padding-top: 0;
	padding-right: 0;
	padding-bottom: 0;
	padding-left: 0;
	font-size: 1em;
}

.chart-legend ul {
	list-style: none;
	width: 50%;
	margin: 0 auto;
}

.chart-legend li {
	text-indent: 0px;
	line-height: 24px;
	position: relative;
	font-weight: 200;
	display: block;
	float: left;
	width: 50%;
	font-size: 0.8em;
}

.chart-legend  li:before {
	display: block;
	width: 10px;
	height: 16px;
	position: absolute;
	left: 0;
	top: 3px;
	content: "";
}

.t1:before {
	background-color: #c62f29;
}

.t2:before {
	background-color: #0033CC;
}

caption {
	text-align: center;
}
</style>
