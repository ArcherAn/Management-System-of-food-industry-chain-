<extend name="Myapp/Tpl/Public/model.html"/>
<block name="title"><title>大棚室内环境基本信息修改</title></block>
<block name="content">
    <div style="margin-top:1%">
        <form action="__APP__/Seedlings/Shed/updateshedindoor" method="post">
            <table class="table table-bordered">
                <tr style="height:45px">
                    <td colspan="5" align="center" bgcolor="#E9E9E9">大棚土壤信息修改</td>
                </tr>
                <tr style="height:45px">
                    <td style="width: 150px;background-color:#E9E9E9">大棚编号</td>
                    <td ><input class="form-control"  style="width: 250px;height: 30px" type="text" name='shedid'  value="<{$data.shedid}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">光编号</td>
                    <td><input class="form-control" style="width: 250px;height: 30px" type="text" name='lightid'value="<{$data.lightid}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">温度编号</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='tempid'  value="<{$data.tempid}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">水编号</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='waterid'  value="<{$data.waterid}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color: #E9E9E9">气编号</td>
                    <td><input class="form-control" style="width: 250px;height: 30px" type="text" name='gasid'  value="<{$data.gasid}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">日期</td>
                    <td><input class="form-control" style="width: 250px;height: 30px" type="text" name='date'  value="<{$data.date}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9"></td>
                    <td><input type='hidden' name='id' value="<{$data.id}>"/><input class="btn btn-primary" type="submit" value="确认修改" onclick="return confirm('是否确认？')"/></td>
                </tr>
            </table>
        </form>
    </div>
</block>
