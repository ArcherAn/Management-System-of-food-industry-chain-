<extend name="Myapp/Tpl/Public/model.html"/>
<block name="title"><title>粮食营养成分信息</title></block>
<block name="content">
    <script>
        function jump(){
            window.location="__APP__/Warehouse/Warehousein/addnutrition";
        }
    </script>
    <div>
        <div style="height: 50px">
            <span style="margin-top: 5px;font-size: 28px">阅读器管理</span><span style="font-size: 14px;margin-top: 3px;">&nbsp;仓储管理平台</span>
        </div>
        <div style="background-color: #b1bab7;height: 30px">
            <span style="margin-top: 5px;float: left">主页&nbsp;>&nbsp;仓储&nbsp;>&nbsp;入库管理</span>
        </div>
        <div  style="margin-top: 10px;">
            <form action="__APP__/Warehouse/Warehousein/nutrition" method="get">
                <span style="font-size: 16px;float: left">营养成分编号：</span>
                <span style="float: left;margin-left: 10px"><input name="shedid" type="text" placeholder="请输入要搜索的营养成分编号" class="form-control"style="width: 200px;height: 30px"></span>
                <span><button type="submit" class="btn btn-primary btn-sm" style="float: left;margin-left: 10px;">查询</button></span>
                <span><button type="button" class="btn btn-success btn-sm" style="float: left;margin-left: 10px" onclick="jump()">新增信息</button></span>
            </form>
        </div>
        <div>
            <table  class="table table-striped table-bordered" style="margin-top: 50px;align:center;">
                <thead>
                <tr>
                    <th>营养成分编号</th>
                    <th>指标1</th>
                    <th>指标2</th>
                    <th>指标3</th>
                    <th>指标4</th>
                    <th>指标5</th>
                    <th>日期</th>
                    <th>操作</th>
                </tr>
                </thead>
                <tbody>
                <volist name='data' id='vo'>
                    <tr>
                        <td><{$vo.nutritionid}>号</td>
                        <td><{$vo.index1}></td>
                        <td><{$vo.index2}></td>
                        <td><{$vo.index3}></td>
                        <td><{$vo.index4}></td>
                        <td><{$vo.index5}></td>
                        <td><{$vo.date}></td>
                        <td><a href="__APP__/Warehouse/Warehousein/modifynutrition/id/<{$vo.id}>">修改</a>|
                            <a href="__APP__/Warehouse/Warehousein/deletenutrition/id/<{$vo.id}>"onclick="return confirm('是否删除？')">删除</a>
                        </td>
                    </tr>
                </volist>
                </tbody>
            </table>
        </div>
        <div align="center"><{$page}></div>
    </div>
</block>
