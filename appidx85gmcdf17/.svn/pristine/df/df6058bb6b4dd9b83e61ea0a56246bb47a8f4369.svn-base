<extend name="Myapp/Tpl/Public/model.html"/>
<block name="title"><title>入库粮食质量信息</title></block>
<block name="content">
    <script>
        function jump(){
            window.location="__APP__/Warehouse/Warehousein/addquality";
        }
    </script>
    <div>
        <div style="height: 50px">
            <span style="margin-top: 5px;font-size: 28px">阅读器管理</span><span style="font-size: 14px;margin-top: 3px;">&nbsp;仓储管理平台</span>
        </div>
        <div style="background-color: #b1bab7;height: 30px">
            <span style="margin-top: 5px;float: left">主页&nbsp;>&nbsp;仓储&nbsp;>&nbsp;入库管理&nbsp;>&nbsp;入库粮食质量信息</span>
        </div>
        <div  style="margin-top: 10px;">
            <form action="__APP__/Warehouse/Warehousein/quality" method="get">
                <span style="font-size: 16px;float: left">入库编号：</span>
                <span style="float: left;margin-left: 10px"><input name="warehouseinid" type="text" placeholder="请输入要搜索的仓库编号" class="form-control"style="width: 200px;height: 30px"></span>
                <span><button type="submit" class="btn btn-primary btn-sm" style="float: left;margin-left: 10px;">查询</button></span>
                <span><button type="button" class="btn btn-success btn-sm" style="float: left;margin-left: 10px" onclick="jump()">新增信息</button></span>
            </form>
        </div>
        <div>
            <table  class="table table-striped table-bordered" style="margin-top: 50px;align:center;">
                <thead>
                <tr>
                    <th>入库编号</th>
                    <th>理化特性编号</th>
                    <th>营养成分编号</th>
                    <th>霉变</th>
                    <th>虫害</th>
                    <th>原料等级</th>
                    <th>日期</th>
                    <th>操作</th>
                </tr>
                </thead>
                <tbody>
                <volist name='data' id='vo'>
                    <tr>
                        <td><{$vo.warehouseinid}>号</td>
                        <td><{$vo.propertyid}></td>
                        <td><{$vo.nutritionid}></td>
                        <td><{$vo.mildew}></td>
                        <td><{$vo.insectpest}></td>
                        <td><{$vo.level}></td>
                        <td><{$vo.date}></td>
                        <td><a href="__APP__/Warehouse/Warehousein/modifyquality/id/<{$vo.id}>">修改</a>|
                            <a href="__APP__/Warehouse/Warehousein/deletequality/id/<{$vo.id}>"onclick="return confirm('是否删除？')">删除</a>
                        </td>
                    </tr>
                </volist>
                </tbody>
            </table>
        </div>
        <div align="center"><{$page}></div>
    </div>
</block>
