<extend name="Myapp/Tpl/Public/model.html"/>
<block name="title"><title>大棚土壤信息修改</title></block>
<block name="content">
    <div style="margin-top:1%">
        <form action="__APP__/Seedlings/Shed/updateshedsoil" method="post">
            <table class="table table-bordered">
                <tr style="height:45px">
                    <td colspan="5" align="center" bgcolor="#E9E9E9">大棚土壤信息修改</td>
                </tr>
                <tr style="height:45px">
                    <td style="width: 150px;background-color:#E9E9E9">大棚编号</td>
                    <td ><input class="form-control"  style="width: 250px;height: 30px" type="text" name='shedid'  value="<{$data.shedid}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">土温</td>
                    <td><input class="form-control" style="width: 250px;height: 30px" type="text" name='soiltemp'value="<{$data.soiltemp}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">土湿</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='soilhumidity'  value="<{$data.soilhumidity}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">PH值</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='PH'  value="<{$data.PH}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color: #E9E9E9">电堆</td>
                    <td><input class="form-control" style="width: 250px;height: 30px" type="text" name='Electricpile'  value="<{$data.Electricpile}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color: #E9E9E9">矿物质1</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='minerals1'  value="<{$data.minerals1}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color: #E9E9E9">矿物质2</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='minerals2'  value="<{$data.minerals2}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">矿物质3</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='minerals3'  value="<{$data.minerals3}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">测量日期</td>
                    <td><input class="form-control" style="width: 250px;height: 30px" type="text" name='date'  value="<{$data.date}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9"></td>
                    <td><input type='hidden' name='id' value="<{$data.id}>"/><input class="btn btn-primary" type="submit" value="确认修改" onclick="return confirm('是否确认？')"/></td>
                </tr>
            </table>
        </form>
    </div>
</block>
