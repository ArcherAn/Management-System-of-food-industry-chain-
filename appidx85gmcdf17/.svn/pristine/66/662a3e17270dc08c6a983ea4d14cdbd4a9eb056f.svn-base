<extend name="Myapp/Tpl/Public/model.html"/>
<block name="title"><title>信息修改</title></block>
<block name="content">
    <div style="margin-top:1%">
        <form action="__APP__/Plant/Product/updateProduction" method="post">
            <table class="table table-bordered">
                <tr style="height:45px">
                    <td colspan="5" align="center" bgcolor="#E9E9E9">农机需求信息修改</td>
                </tr>
                <tr style="height:45px">
                    <td style="width: 150px;background-color:#E9E9E9">农田编号</td>
                    <td ><input class="form-control" style="width: 250px;height: 30px" type="text" name='farmlandid'  value="<{$data.farmlandid}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="width: 150px;background-color:#E9E9E9">收割编号</td>
                    <td ><input class="form-control" style="width: 250px;height: 30px" type="text" name='productionid'  value="<{$data.productionid}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">秧苗批次号</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='seedid'value="<{$data.seedid}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">产量</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='production'  value="<{$data.production}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">收获日期</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='harvestdate'  value="<{$data.harvestdate}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9">负责人</td>
                    <td><input class="form-control"  style="width: 250px;height: 30px" type="text" name='head'  value="<{$data.head}>"/></td>
                </tr>
                <tr style="height:45px">
                    <td style="background-color:#E9E9E9"></td>
                    <td><input type='hidden' name='id' value="<{$data.id}>"/><input class="btn btn-primary" type="submit" value="确认修改" onclick="return confirm('是否确认？')"/></td>
                </tr>
            </table>
        </form>
    </div>
</block>
