<extend name="Myapp/Tpl/Public/model.html"/>
<block name="title"><title>浸种操作信息</title></block>
<block name="content">
<script>
document.getElementById('yuyang').click();
</script>
    <script>
        function jump(){
            window.location="__APP__/Seedlings/Seed/addsoakoperate";
        }
    </script>
    <div>
        <div style="height: 50px">
            <span style="margin-top: 5px;font-size: 28px">育秧管理平台</span>
        </div>
        <div style="background-color: #b1bab7;height: 30px">
            <span style="margin-top: 5px;float: left">主页&nbsp;>&nbsp;育秧&nbsp;>&nbsp;种子管理&nbsp;>&nbsp;浸种操作信息</span>
        </div>
        <div  style="margin-top: 10px;">
            <form action="__APP__/Seedlings/Seed/soakoperate" method="get">
                <span style="font-size: 16px;float: left">种子批次号：</span>
                <span style="float: left;margin-left: 10px"><input name="seedid" type="text" placeholder="请输入要搜索的种子批次号" class="form-control"style="width: 200px;height: 30px"></span>
                <span><button type="submit" class="btn btn-primary btn-sm" style="float: left;margin-left: 10px;">查询</button></span>
                <span><button type="button" class="btn btn-success btn-sm" style="float: left;margin-left: 10px" onclick="jump()">新增信息</button></span>
            </form>
        </div>
        <div>
            <table  class="table table-striped table-bordered" style="margin-top: 50px;align:center;">
                <thead>
                <tr>
                    <th>种子批次号</th>
                    <th>药品批次号</th>
                    <th>药品浓度</th>
                    <th>起始日期</th>
                    <th>截止日期</th>
                    <th>操作员</th>
                    <th>操作</th>
                </tr>
                </thead>
                <tbody>
                <volist name='data' id='vo'>
                    <tr>
                        <td><{$vo.seedid}></td>
                        <td><{$vo.drugid}></td>
                        <td><{$vo.concentration}>ml/L</td>
                        <td><{$vo.startdate}></td>
                        <td><{$vo.enddate}></td>
                        <td><{$vo.operator}></td>
                        <td><a href="__APP__/Seedlings/Seed/modifysoakoperate/id/<{$vo.id}>">修改</a>|
                            <a href="__APP__/Seedlings/Seed/deletesoakoperate/id/<{$vo.id}>"onclick="return confirm('是否删除？')">删除</a>
                        </td>
                    </tr>
                </volist>
                </tbody>
            </table>
        </div>
        <div align="center"><{$page}></div>
    </div>
</block>
