<extend name="Myapp/Tpl/Public/model.html"/>
<block name="title"><title>仓库粮食监控理化特性信息修改</title></block>
<block name="content">
        <div style="margin-top:1%">
            <form action="__APP__/Warehouse/Warehousemonitor/updateproperty" method="post">
                <table class="table table-bordered" style="border: 1px solid red">
                    <tr style="height:45px">
                        <td colspan="5" align="center" bgcolor="#E9E9E9">仓库粮食监控理化特性信息修改</td>
                    </tr>
                    <tr style="height:45px">
                        <td style="width: 150px;background-color:#E9E9E9">理化特性编号</td>
                        <td ><input class="form-control" placeholder="如：1" style="width: 250px;height: 30px" type="text" name='propertyid'  value="<{$data.propertyid}>"/></td>
                    </tr>
                    <tr style="height:45px">
                        <td style="background-color:#E9E9E9">指标1</td>
                        <td><input class="form-control" placeholder="如：？" style="width: 250px;height: 30px" type="text" name='index1'value="<{$data.index1}>"/></td>
                    </tr>
                    <tr style="height:45px">
                        <td style="background-color:#E9E9E9">指标2</td>
                        <td><input class="form-control" placeholder="如：？" style="width: 250px;height: 30px" type="text" name='index2'  value="<{$data.index2}>"/></td>
                    </tr>
                    <tr style="height:45px">
                        <td style="background-color:#E9E9E9">指标3</td>
                        <td><input class="form-control" placeholder="如：？" style="width: 250px;height: 30px" type="text" name='index3'  value="<{$data.index3}>"/></td>
                    </tr>
                    <tr style="height:45px">
                        <td style="background-color: #E9E9E9">指标4</td>
                        <td><input class="form-control" placeholder="如：？" style="width: 250px;height: 30px" type="text" name='index4'  value="<{$data.index4}>"/></td>
                    </tr>
                    <tr style="height:45px">
                        <td style="background-color: #E9E9E9">日期</td>
                        <td><input class="form-control" placeholder="如：2015-03-18" style="width: 250px;height: 30px" type="text" name='date'  value="<{$data.date}>" onClick="WdatePicker()"/></td>
                    </tr>
                    <tr style="height:45px">
                        <td style="background-color:#E9E9E9"></td>
                        <td><input type='hidden' name='id' value="<{$data.id}>"/><input class="btn btn-primary" type="submit" value="确认修改" onclick="return confirm('是否确认？')"/></td>
                    </tr>
                </table>
            </form>
        </div>
    </block>
